package com.ibm.client.cui;

//import java.sql.*;
//import java.util.Date;

class Point
{
	public int x;
	public int y;
}

public class MainApp {

//	Date date;
	static Point point1,point2,point3;
	public static void main(String[] args)
	{
//		System.out.println("Hello IBM,Welcome to Banglore");
		
		
		
		point1 = new Point();
		point1.x=2;
		point1.y=4;
		point2 = point1;
		point2.x = 3;
		point2.y = 6;
		point3.x=10;
		
		System.out.println(point1.x+","+point1.y);
		System.out.println(point2.x+","+point2.y);

	}
}
